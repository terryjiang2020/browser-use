# Copy this file to .env and fill in your actual values

# OpenAI Configuration (choose one LLM provider)
OPENAI_API_KEY=sk-your_openai_api_key_here
OPENAI_MODEL=gpt-4o

# Anthropic Configuration (alternative to OpenAI)
ANTHROPIC_API_KEY=your_anthropic_api_key_here
ANTHROPIC_MODEL=claude-3-5-sonnet-20241022

# DeepSeek Configuration (alternative to OpenAI)
DEEPSEEK_API_KEY=your_deepseek_api_key_here
DEEPSEEK_MODEL=deepseek-reasoner

# Other LLM providers (optional)
AZURE_OPENAI_ENDPOINT=
AZURE_OPENAI_KEY=
GOOGLE_API_KEY=
GROK_API_KEY=
NOVITA_API_KEY=

# AWS Configuration (REQUIRED for SQS/S3 integration)
AWS_ACCESS_KEY_ID=AKIA...
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_REGION=us-east-1

# AWS SQS Queue URL (REQUIRED)
# Format: https://sqs.{region}.amazonaws.com/{account-id}/{queue-name}
AWS_SQS_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/123456789012/browser-automation-queue

# AWS S3 Bucket (REQUIRED)
AWS_S3_BUCKET=your-browser-automation-bucket
AWS_S3_REGION=us-east-1

# API Base URL (REQUIRED)
# The base URL of your main application API
# Example: https://api.yourdomain.com/v1
API_BASE_URL=https://api.yourdomain.com/v1

# Application Settings
LOG_LEVEL=INFO
MAX_CONCURRENT_TASKS=5
DEFAULT_TIMEOUT=300

# Webhook Settings
WEBHOOK_TIMEOUT=30
